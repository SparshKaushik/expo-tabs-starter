{
  "name": "my-expo-app",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "APP_ENV=development expo start -d",
    "android": "APP_ENV=development expo run:android",
    "ios": "APP_ENV=development expo run:ios",
    "web": "APP_ENV=development expo start --web",
    "doctor": "bunx --bun expo-doctor@latest",
    "build:development:ios": "APP_ENV=development eas build --profile development --platform ios",
    "build:development:android": "APP_ENV=development eas build --profile development --platform android",
    "build:production:ios": "APP_ENV=production eas build --profile production --platform ios",
    "build:production:android": "APP_ENV=production eas build --profile production --platform android",
    "prebuild": "expo prebuild",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "banner": "bunx app-icon-badge ./assets/images/icon.png --type=ribbon --background=#ff0000 --color=white --text=DEV"
  },
  "dependencies": {
    "@bottom-tabs/react-navigation": "^0.8.9",
    "@expo/vector-icons": "^14.0.0",
    "@react-navigation/native": "^7.0.3",
    "@t3-oss/env-core": "^0.12.0",
    "expo": "^52.0.38",
    "expo-constants": "~17.0.8",
    "expo-linking": "~7.0.5",
    "expo-navigation-bar": "^4.0.8",
    "expo-router": "~4.0.6",
    "expo-splash-screen": "^0.29.22",
    "expo-status-bar": "~2.0.1",
    "expo-system-ui": "~4.0.8",
    "expo-web-browser": "~14.0.2",
    "nativewind": "latest",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.7",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "^3.17.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-web": "~0.19.10",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/bun": "latest",
    "@types/react": "~18.3.12",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "eslint": "^8.57.0",
    "eslint-config-expo": "~8.0.1",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.0",
    "typescript": "~5.3.3"
  },
  "private": true
}
